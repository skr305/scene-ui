{
  "name": "scene-ui",
  "private": false,
  "version": "0.3.13",
  "description": "a ui library",
  "keywords": ["ui", "front-end", "vue", "ts", "component", "lib", "starter", "vite", "toolkit"],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:lib": "vite build --config ./build/vite.lib.config.ts",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "b-comp": "vite build --config build/comp.config.ts"
  },
  "main": "./dist/scene-ui.umd.js",
  "module": "./dist/scene-ui.es.js",
  "exports": {
    ".": {
      "import": "./dist/scene-ui.es.js",
      "require": "./dist/scene-ui.umd.js"
    },
    "./style": {
      "import": "./dist/index.css",
      "require": "./dist/index.css"
    },
    "./*": {
      "import": "./*",
      "require": "./*"
    },
    "./dist/*": {
      "import": "./dist/*",
      "require": "./dist/*"
    }
  },
  "dependencies": {
    "@types/events": "^3.0.0",
    "@types/lodash": "^4.14.180",
    "@types/vue-router": "^2.0.0",
    "less": "^4.1.2",
    "lodash": "^4.17.21",
    "ts-loader": "^9.2.7",
    "vite": "^2.9.1",
    "vite-plugin-dts": "^1.0.5",
    "vite-svg-loader": "^3.2.0",
    "vue": "^3.2.25",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/vue3": "^6.4.19",
    "@vitejs/plugin-vue": "^2.2.0",
    "babel-loader": "^8.2.3",
    "ts-loader": "^9.2.8",
    "tslint": "^6.1.3",
    "tslint-loader": "^3.5.4",
    "typescript": "^4.6.3",
    "vite": "^2.8.0",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^0.29.8"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "sideEffects": [
    "dist/*.svg",
    "dist/*.css",
    "dist/*.ico",
    "./dist/*.svg",
    "./dist/*.css",
    "./dist/*.ico",
    "./*.css",
    "./*.svg",
    "./index.css",
    "./dist/index.css",
    "./dist/*",
    "./dist/right_16.svg"
  ]
}
